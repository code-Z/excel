<h2>Import Excel File</h2>
<%= form_tag import_workbooks_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<p>
  Download:
  <%= link_to "CSV", workbooks_path(format: "csv") %> |
  <%= link_to "Excel", workbooks_path(format: "xls") %>
</p>

<%= @row %>